<block>
    <name>Reed Solomon Dec isdbt</name>
    <key>oneseg_reed_solomon_dec_isdbt</key>
    <category>oneseg</category>
    <import>import oneseg</import>
    <make>oneseg.reed_solomon_dec_isdbt()</make>
    <sink>
        <name>in</name>
        <type>byte</type>
        <vlen>1*(255-51)</vlen>
    </sink>
    <source>
        <name>out</name>
        <type>byte</type>
        <vlen>1*(239-51)</vlen>
    </source>

  <!-- optional outputs-->
    <source>
        <name>ber</name>
        <type>float</type>
        <vlen>1</vlen>
        <optional>1</optional>
    </source>
</block>
